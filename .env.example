# Google API 設定
GOOGLE_API_KEY=your_google_api_key_here

# Neo4j 數據庫設定
NEO4J_URI=neo4j://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password_here

# MCP 設定 (可選)
ENABLE_MCP=false
MCP_FILE_SERVER_URL=http://localhost:3001/events
MCP_FILE_SERVER_TOKEN=your_token_here

# Flask 設定
FLASK_PORT=5001
FLASK_DEBUG=false

